    "body-parser": "^1.19.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "pg": "^7.18.1",
    "xss-clean": "^0.1.1"
